@model MaestroDetalleFJCO20241103.Models.Computadora

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<h3>Seguro de eliminarlo?</h3>
<div>
    <h4>Computadora</h4>
   
    <form asp-action="Delete">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row">
            <div class="form-group col-md-6">
                <label asp-for="Nombre" class="control-label"></label>
                <input asp-for="Nombre" readonly class="form-control" />
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </div>
            <div class="form-group col-md-3">
                <label asp-for="Marca" class="control-label"></label>
                <input asp-for="Marca" readonly class="form-control" />
                <span asp-validation-for="Marca" class="text-danger"></span>
            </div>
            <div class="form-group col-md-3">
                <label asp-for="Precio" class="control-label"></label>
                <input asp-for="Precio" readonly class="form-control" />
                <span asp-validation-for="Precio" class="text-danger"></span>
            </div>
        </div>
        <div class="row row-cols-md-12">
            @await Html.PartialAsync("_DetCom", Model.Componente)
        </div>
        <div class="row">
            <div class="form-group">
                <input type="submit" value="Eliminar" class="btn btn-danger" />
                <a class="btn btn-secondary" asp-action="Index">Regresar</a>
            </div>
        </div>
    </form>
</div>
